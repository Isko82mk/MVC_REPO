@model Book
<div class="card" style="width: 18rem; ">
   
    <img src="~/img/BooksImg/@(Model.Name).png" class="card-img-top" alt="img">
    <ul class="list-group list-group-flush">
        
        <li class="list-group-item">Author: @Model.Author</li>
        <li class="list-group-item">Rent Price: @Model.Price</li>
        <li class="list-group-item">Genre: @Model.Genre</li>
    
    </ul>

</div>






















